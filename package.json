{
  "name": "react-swipeable-list",
  "description": "Swipeable list component for React",
  "version": "1.10.0",
  "author": {
    "name": "Marek Rozmus",
    "url": "https://github.com/marekrozmus/react-swipeable-list/graphs/contributors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marekrozmus/react-swipeable-list"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "swipe",
    "swipeable",
    "list",
